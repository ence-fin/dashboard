{
  "$schema": "https://turbo.build/schema.json",
  "globalDotEnv": [".env"],
  "pipeline": {
    "build": {
      "dotEnv": [".env"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "dotEnv": [".env"],
      "cache": false,
      "persistent": true
    },
    "type-check": {}
  }
}
